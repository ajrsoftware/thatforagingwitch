{
    "name": "ajrsoftware/thatforagingwitch",
    "type": "wordpress-theme",
    "authors": [
        {
            "name": "Ashley Redman",
            "email": "ashley.redman@ajrsoftware.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org",
            "only": [
                "wpackagist-plugin/*",
                "wpackagist-theme/*"
            ]
        }
    ],
    "extra": {
        "installer-paths": {
            "lib/{$name}/": [
                "type:wordpress-plugin"
            ]
        }
    },
    "require": {
        "composer/installers": "^1.0",
        "rareloop/lumberjack-core": "^6.0",
        "wpackagist-plugin/advanced-custom-fields": "^6.1"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
